      <Label layoutX="49.0" layoutY="236.0" text="máximo de dias de reserva (Professor)" />
      <TextField fx:id="txtMaxDaysProfessor" layoutX="49.0" layoutY="255.0" prefHeight="25.0" prefWidth="239.0" promptText="Digite o tempo máximo de reserva" />
      <Label layoutX="49.0" layoutY="46.0" text="tipo de peça" />
      <TextField fx:id="txtPartType" layoutX="49.0" layoutY="65.0" prefHeight="25.0" prefWidth="239.0" promptText="Digite o tipo de peça" />
